corridor[3].promotion.rule,
  require LifeforceDecision != DenyAfterBloodGuard &&
          BrainSpecs.driftsafe == true

selection.criteria.order,
  1) corridor.priority asc
  2) observed.RoH.mean asc
  3) knowledge.factor.desc
