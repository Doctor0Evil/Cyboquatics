corridor[2].promotion.rule,
  require DraculaWaveDecision in {Safe,Brake} &&
          RollbackRequired.count == 0

corridor[3].id,lifeforce-envelope.v1
corridor[3].kind,meta-lifeforce
corridor[3].priority,0
corridor[3].metrics,lifeforce.integrity,lifeforce.chi,EEG.load,HRV.sdnn,IL6
